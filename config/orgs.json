{
  "TLS": false,
  "docker": {
    "fabricTag": "1.3.0",
    "thirdPartyTag": "0.4.13",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": {
        "dir": "config/configtx"
      },
      "MSPROOT": {
        "dir": "config/ca-crypto-config/"
      }
    }
  },
  "orderer": {
    "type": "kafka",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "solo": {
      "orgName": "Delphi.com",
      "container_name": "orderer",
      "portHost": 7050,
      "ca": {
        "portHost": 7454
      },
      "mspid": "OrdererMSP"
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "orgs": {
        "ICDD.ASTRI.org": {
          "mspid": "ICDDMSP",
          "ca": {
            "portHost": 7454
          },
          "orderers": {
            "orderer0": {
              "portHost": 7050
            },
            "orderer1": {
              "portHost": 8050
            },
            "orderer2": {
              "portHost": 9050,
              "stateVolume": "Documents/backupVolumes/orderer2"
            }
          }
        }
      }
    }
  },
  "channels": {
    "allchannel": {
      "file": "all.tx",
      "eventWaitTime": 45000,
      "orgs": {
        "ASTRI.org": {
          "peerIndexes": [
            0,
            1
          ]
        },
        "icdd": {
          "peerIndexes": [
            0,
            1
          ]
        }
      }
    }
  },
  "orgs": {
    "ASTRI.org": {
      "peers": [
        {
          "container_name": "peer0.ASTRI.org",
          "port": 7051
        },
        {
          "container_name": "peer1.ASTRI.org",
          "port": 7151
        }
      ],
      "ca": {
        "portHost": 7054
      },
      "mspid": "ASTRIMSP"
    },
    "icdd": {
      "peers": [
        {
          "container_name": "peer0.icdd",
          "port": 8051
        },
        {
          "container_name": "peer1.icdd",
          "port": 8151
        }
      ],
      "ca": {
        "portHost": 8054
      },
      "mspid": "icdd"
    }
  }
}
