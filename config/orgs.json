{
  "TLS": true,
  "docker": {
    "fabricTag": "2.0.0-alpha",
    "caTag": "2.0.0-alpha",
    "thirdPartyTag": "0.4.15",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": "Documents/delphi-fabric/config/configtx",
      "MSPROOT": "Documents/delphi-fabric/config/ca-crypto-config/"
    }
  },
  "orderer": {
    "type": "solo",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "solo": {
      "orgName": "Delphi.com",
      "container_name": "orderer",
      "operations": {
        "port": 8443,
        "metrics": "prometheus"
      },
      "portHost": 7050,
      "ca": {
        "portHost": 7454
      },
      "mspid": "OrdererMSP"
    },
    "etcdraft": {
      "orgs": {
        "ICDD.ASTRI.org": {
          "mspid": "ICDDMSP",
          "ca": {
            "portHost": 7454
          },
          "orderers": {
            "orderer0": {
              "portHost": 7050
            },
            "orderer1": {
              "portHost": 8050
            },
            "orderer2": {
              "portHost": 9050
            }
          }
        }
      }
    }
  },
  "channels": {
    "allchannel": {
      "file": "all.tx",
      "orgs": {
        "ASTRI.org": {
          "peerIndexes": [
            0,
            1
          ]
        },
        "icdd": {
          "peerIndexes": [
            0,
            1
          ]
        }
      }
    }
  },
  "orgs": {
    "ASTRI.org": {
      "peers": [
        {
          "container_name": "peer0.ASTRI.org",
          "port": 7051,
          "operations": {
            "port": 9443
          },
          "couchDB": {
            "container_name": "couchdb.peer0.ASTRI.org",
            "port": 7984
          }
        },
        {
          "container_name": "peer1.ASTRI.org",
          "port": 7151
        }
      ],
      "ca": {
        "portHost": 7054
      },
      "mspid": "ASTRIMSP"
    },
    "icdd": {
      "peers": [
        {
          "container_name": "peer0.icdd",
          "port": 8051
        },
        {
          "container_name": "peer1.icdd",
          "port": 8151
        }
      ],
      "ca": {
        "portHost": 8054
      },
      "mspid": "icddMSP"
    }
  }
}
